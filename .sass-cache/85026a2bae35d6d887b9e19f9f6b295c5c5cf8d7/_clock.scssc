.clock #minute {
  width: 0;
  height: 0;
  position: absolute;
  top: 50%;
  left: 50%;
  margin: -35% -1px 0;
  padding: 35% 1px 0;
  background: $clockColor;
  -webkit-transform-origin: 50% 100%;
  -moz-transform-origin: 50% 100%;
  -ms-transform-origin: 50% 100%;
  transform-origin: 50% 100%;
  border-radius: 5px 5px 0 0;
}


.clock #second {

  width:.4em;
  height:14em;
  border-radius:.1em .1em 0 0/10em 10em 0 0;
  background:#c00;
  position:absolute;
  bottom:50%; left:50%;
  margin:0 0 -2em -0.1em;
  // box-shadow:rgba(255,255,255,.4) 0 0 .4em;
  @include transform-origin(0.1em, 12em);
  // @include transition(all ease-in-out .3s)
}
.clock #second:after {
  content:'';
  width:1.4em;
  height:1.4em;
  border-radius:.7em;
  background:inherit;
  position:absolute;
  left:-.65em; 
  bottom:1.35em;
}
.clock #second:before {
  content:'';
  width:.8em;
  height:3em;
  border-radius:.2em .2em .4em .4em/.2em .2em 2em 2em;
  // box-shadow:rgba(0,0,0,.8) 0 0 .2em;
  background:inherit;
  position:absolute;
  left:-.2em; bottom:-3em;
}

