.ampm{
  margin-top: -3.4em;
  margin-bottom: 2.4em;
  font-size: 12px;
  // color:rgb(0,255,0);
}

.clock .minute-marks li {
  display:block;
  width:.2em;
  height:.6em;
  background:white;
  position:absolute;
  top:50%; left:50%;

  // margin:-.4em 0 0 -.1em;
}


.clock .digits li span{
  font-size: 9px;
}
.clock .digits li {
  font-size:1.6em;
  display:block;
  width:1.6em;
  height:1.6em;
  position:absolute;
  top:50%; left:50%;
  line-height:1.6em;
  text-align:center;
  margin:-.8em 0 0 -.7em;
  font-weight:bold;
}

.clock .digits li:nth-child(1) { @include transform(translate(3.9em, -6.9em))}
.clock .digits li:nth-child(2) { @include transform(translate(6.9em, -4em))}
.clock .digits li:nth-child(3) { @include transform(translate(8em, 0em))}
.clock .digits li:nth-child(4) { @include transform(translate(6.8em, 4em))}
.clock .digits li:nth-child(5) { @include transform(translate(3.9em, 6.9em))}
.clock .digits li:nth-child(6) { @include transform(translate(0em, 8em))}
.clock .digits li:nth-child(7) { @include transform(translate(-3.9em, 6.9em))}
.clock .digits li:nth-child(8) { @include transform(translate(-6.8em, 4em))}
.clock .digits li:nth-child(9) { @include transform(translate(-8em, 0em))}
.clock .digits li:nth-child(10) { @include transform(translate(-6.9em, -4em))}
.clock .digits li:nth-child(11) { @include transform(translate(-3.9em, -6.9em))}
.clock .digits li:nth-child(12) { @include transform(translate(0em, -8em))}


